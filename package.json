{
  "name": "whinepad",
  "version": "1.0.0",
  "description": "Sample React Application.",
  "main": "bundle.js",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.6",
    "create-react-class": "^15.6.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.20.0",
    "invariant": "^2.2.4",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.13.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "react/jsx-uses-react": 1
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_moduels/react",
      "node_modules/react-dom",
      "node_modules/react-addons-rest-utils",
      "node_modules/fbjs"
    ]
  },
  "scripts": {
    "test": "jest",
    "build": "sh scripts/build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HiroshiHara/whinepad.git"
  },
  "author": "Hiroshi Hara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HiroshiHara/whinepad/issues"
  },
  "homepage": "https://github.com/HiroshiHara/whinepad#readme"
}
