{
  "name": "whinepad",
  "version": "1.0.0",
  "description": "Sample React Application.",
  "main": "bundle.js",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "create-react-class": "^15.6.3",
    "react-dom": "^16.13.1",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.20.0"
  },
  "//": "below property are arguments for babel sciprt in scripts/build.sh .",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "react/jsx-uses-react": 1
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "sh scripts/build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HiroshiHara/whinepad.git"
  },
  "author": "Hiroshi Hara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HiroshiHara/whinepad/issues"
  },
  "homepage": "https://github.com/HiroshiHara/whinepad#readme"
}
